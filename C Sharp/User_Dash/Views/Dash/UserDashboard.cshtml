@{ ViewData["Title"] = "User Dashboard"; }
<div class="col-lg-12">
    
    <h2>All Users</h2>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Email</th>
                <th>Created At</th>
                <th>User Level</th>
            </tr>
        </thead>
        <tbody>
            @{ foreach( var user in @ViewBag.AllUsers ){
            <tr>
                <td>@user.UserId</td>
                <td><a href="/Profile/@ViewBag.User.UserId">@user.FirstName @user.LastName</a></td>
                <td>@user.Email</td>
                <td>@user.CreatedAt.ToString("MMM dd, yyyy")</td>
                <td>@if(user.UserLevel == 9) {
                    <text>Admin</text>} else{
                    <text>Normal</text>}
                </td>
            </tr>
            }}

        </tbody>
    </table>
</div>